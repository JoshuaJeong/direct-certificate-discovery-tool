<?xml version="1.0" encoding="UTF-8"?>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
    xmlns:fn="http://java.sun.com/jsp/jstl/functions"
    xmlns:jsp="http://java.sun.com/JSP/Page"
    xmlns:spring="http://www.springframework.org/tags"
    xmlns:spring-form="http://www.springframework.org/tags/form"
    xmlns:x="http://java.sun.com/jsp/jstl/xml">
    <jsp:directive.page contentType="text/html; UTF-8" isELIgnored="false" pageEncoding="UTF-8"/>
    <jsp:output doctype-root-element="html" doctype-system="about:legacy-compat" omit-xml-declaration="true"/>
    <head>
        <jsp:directive.include file="include/head.jsp"/>
        <link rel="stylesheet" type="text/css" href="${urlStaticStyles}/error.css"/>
        <title><spring:message code="dcdt.web.title.error"/></title>
    </head>
    <body>
        <jsp:directive.include file="include/nav.jsp"/>
        <div id="content" class="container">
            <h2><spring:message code="dcdt.web.heading.error"/></h2>
            <code id="error-stack-trace">${errorStackTrace}</code>
        </div>
        <jsp:directive.include file="include/footer.jsp"/>
    </body>
</html>